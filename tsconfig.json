{
  "compilerOptions": {
    "module": "commonjs",
    "noEmit": true,
    "downlevelIteration": true,
    "lib": [
      "esnext"
    ],
    "types": [
      "node", "jasmine", "./spec"
    ],
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": true
  }
}
