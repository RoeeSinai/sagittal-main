{
  "name": "sagittal-calculator",
  "version": "1.0.0",
  "description": "an online calculator for the Sagittal notation system",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmloegcmluin/sagittal-calculator.git"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack",
    "profile": "node --prof -r ts-node/register/transpile-only --max-old-space-size=131072 [[script.ts]] && node --prof-process isolate-*.log > processed.txt && rm isolate-*.log",
    "lint": "tslint \"src/**/*.ts\" --fix && tslint \"spec/**/*.ts\" --fix",
    "test": "node -r ts-node/register/transpile-only node_modules/jasmine/bin/jasmine",
    "test-names": "node -r ts-node/register/transpile-only node_modules/jasmine/bin/jasmine --names=true",
    "test-ci": "node -r ts-node/register node_modules/jasmine/bin/jasmine --ci=true",
    "tmp": "node -r ts-node/register/transpile-only src/scripts/tmp.ts",
    "analyze-ji-notation-bound-class": "node -r ts-node/register/transpile-only src/scripts/jiNotationBoundClass/scripts/analyzeBoundClass.ts",
    "analyze-ji-notation-bound-classes": "node -r ts-node/register/transpile-only src/scripts/jiNotationBoundClass/scripts/analyzeBoundClasses.ts",
    "generate-levels-diagram": "node -r ts-node/register/transpile-only src/scripts/jiNotationBoundClass/scripts/generateLevelsDiagram.ts",
    "find-commas": "node -r ts-node/register/transpile-only src/scripts/jiPitch/scripts/findCommas.ts",
    "analyze-ji-pitch": "node -r ts-node/register/transpile-only src/scripts/jiPitch/scripts/analyzeJiPitch.ts",
    "analyze-ji-pitches": "node -r ts-node/register/transpile-only src/scripts/jiPitch/scripts/analyzeJiPitches.ts",
    "zone-commas": "node -r ts-node/register/transpile-only src/scripts/jiPitch/scripts/zoneCommas.ts",
    "solve-best-complexity-metric": "node -r ts-node/register/transpile-only src/scripts/complexityAndBadnessMetricLfc/scripts/solveBestComplexityMetric.ts",
    "perfect-metric-from-scope": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/popularityMetricLfc/scripts/perfectMetricFromScope.ts",
    "antivotes": "node -r ts-node/register/transpile-only src/scripts/popularityMetricLfc/scripts/antivotes.ts",
    "sos": "node -r ts-node/register/transpile-only src/scripts/popularityMetricLfc/scripts/sumOfSquares.ts",
    "solve-best-metrics-for-chunk-count": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/popularityMetricLfc/scripts/solveBestMetricsForChunkCount.ts",
    "filter-rottens": "node -r ts-node/register/transpile-only src/scripts/popularityMetricLfc/scripts/correctGoofs/filterRottens.ts",
    "find-parameter-extrema": "node -r ts-node/register/transpile-only src/scripts/popularityMetricLfc/scripts/findParameterExtrema.ts",
    "perfect-metrics": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/popularityMetricLfc/scripts/perfectMetrics.ts",
    "guess-backfill-spread-dynamic-parameters": "node -r ts-node/register/transpile-only src/scripts/popularityMetricLfc/scripts/correctGoofs/guessBackfillSpreadDynamicParameters.ts",
    "popular-2-3-free-classes": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/popular23FreeClass/scripts/popular23FreeClasses.ts",
    "sorted-numerators": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/popular23FreeClass/scripts/sortedNumerators.ts",
    "late-tina-commas": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/jiPitch/scripts/lateTinaCommas.ts",
    "semitina-occams": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/jiPitch/scripts/semitinaOccams.ts",
    "ji-notation-badness": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/complexityAndBadnessMetricLfc/scripts/jiNotationBadness.ts"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.11",
    "@types/node": "^14.0.20",
    "@types/vexflow": "^3.0.0",
    "colors": "^1.4.0",
    "commander": "^5.1.0",
    "html-webpack-plugin": "^4.5.0",
    "jasmine": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "opentype.js": "^1.3.3",
    "ts-loader": "^8.0.11",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^4.0.2",
    "vexflow": "^3.0.9",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0"
  },
  "engines": {
    "node": ">14"
  }
}
