{
  "name": "sagittal-calculator",
  "version": "1.0.0",
  "description": "an online calculator for the Sagittal notation system",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmloegcmluin/sagittal-calculator.git"
  },
  "scripts": {
    "lint": "tslint \"src/**/*.ts\" --fix && tslint \"spec/**/*.ts\" --fix",
    "test": "node -r ts-node/register/transpile-only node_modules/jasmine/bin/jasmine",
    "test-ci": "node -r ts-node/register node_modules/jasmine/bin/jasmine --ci=true",
    "analyze-bound": "node -r ts-node/register/transpile-only src/scripts/bound/commands/analyzeBound.ts",
    "analyze-bounds": "node -r ts-node/register/transpile-only src/scripts/bound/commands/analyzeBounds.ts",
    "generate-levels-diagram": "node -r ts-node/register/transpile-only src/scripts/bound/commands/generateLevelsDiagram.ts",
    "find-commas": "node -r ts-node/register/transpile-only src/scripts/pitch/commands/findCommas.ts",
    "analyze-rational-pitch": "node -r ts-node/register/transpile-only src/scripts/pitch/commands/analyzeRationalPitch.ts",
    "best-metric-from-scope": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/lfc/commands/bestMetricFromScope.ts",
    "antivotes": "node -r ts-node/register/transpile-only src/scripts/lfc/commands/antivotes.ts",
    "sos": "node -r ts-node/register/transpile-only src/scripts/lfc/commands/sumOfSquares.ts",
    "solve-best-metrics-for-chunk-count": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/lfc/commands/solveBestMetricsForChunkCount.ts",
    "filter-rottens": "node -r ts-node/register/transpile-only src/scripts/lfc/commands/correctGoofs/filterRottens.ts",
    "find-parameter-extrema": "node -r ts-node/register/transpile-only src/scripts/lfc/commands/findParameterExtrema.ts",
    "perfect-metrics": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/lfc/commands/perfectMetrics.ts",
    "guess-backfill-spread-dynamic-parameters": "node -r ts-node/register/transpile-only src/scripts/lfc/commands/correctGoofs/guessBackfillSpreadDynamicParameters.ts",
    "popular-ratios": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/popularRatios/commands/popularRatios.ts",
    "popular-ratios-with-best-notating-commas": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/popularRatios/commands/popularRatiosWithBestNotatingCommas.ts"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.11",
    "@types/node": "^14.0.20",
    "colors": "^1.4.0",
    "commander": "^5.1.0",
    "jasmine": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": ">14"
  }
}
