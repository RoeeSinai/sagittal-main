{
  "name": "sagittal-calculator",
  "version": "1.0.0",
  "description": "an online calculator for the Sagittal notation system",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmloegcmluin/sagittal-calculator.git"
  },
  "scripts": {
    "profile": "node --prof -r ts-node/register/transpile-only --max-old-space-size=131072 [[script.ts]] && node --prof-process isolate-*.log > processed.txt && rm isolate-*.log",
    "lint": "tslint \"src/**/*.ts\" --fix && tslint \"spec/**/*.ts\" --fix",
    "test": "node -r ts-node/register/transpile-only node_modules/jasmine/bin/jasmine",
    "test-names": "node -r ts-node/register/transpile-only node_modules/jasmine/bin/jasmine --names=true",
    "test-ci": "node -r ts-node/register node_modules/jasmine/bin/jasmine --ci=true",
    "tmp": "node -r ts-node/register/transpile-only src/scripts/tmp.ts",
    "analyze-ji-notation-bound-class": "node -r ts-node/register/transpile-only src/scripts/jiNotationBoundClass/commands/analyzeBoundClass.ts",
    "analyze-ji-notation-bound-classes": "node -r ts-node/register/transpile-only src/scripts/jiNotationBoundClass/commands/analyzeBoundClasses.ts",
    "generate-ji-notation-levels-diagram": "node -r ts-node/register/transpile-only src/scripts/jiNotationBoundClass/commands/generateLevelsDiagram.ts",
    "find-commas": "node -r ts-node/register/transpile-only src/scripts/jiPitch/commands/findCommas.ts",
    "analyze-ji-pitch": "node -r ts-node/register/transpile-only src/scripts/jiPitch/commands/analyzeJiPitch.ts",
    "zone-commas": "node -r ts-node/register/transpile-only src/scripts/jiPitch/commands/zoneCommas.ts",
    "primary-comma-usefulness": "node -r ts-node/register/transpile-only src/scripts/usefulnessMetricLfc/commands/primaryCommaUsefulness.ts",
    "perfect-metric-from-scope": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/popularityMetricLfc/commands/perfectMetricFromScope.ts",
    "antivotes": "node -r ts-node/register/transpile-only src/scripts/popularityMetricLfc/commands/antivotes.ts",
    "sos": "node -r ts-node/register/transpile-only src/scripts/popularityMetricLfc/commands/sumOfSquares.ts",
    "solve-best-metrics-for-chunk-count": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/popularityMetricLfc/commands/solveBestMetricsForChunkCount.ts",
    "filter-rottens": "node -r ts-node/register/transpile-only src/scripts/popularityMetricLfc/commands/correctGoofs/filterRottens.ts",
    "find-parameter-extrema": "node -r ts-node/register/transpile-only src/scripts/popularityMetricLfc/commands/findParameterExtrema.ts",
    "perfect-metrics": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/popularityMetricLfc/commands/perfectMetrics.ts",
    "guess-backfill-spread-dynamic-parameters": "node -r ts-node/register/transpile-only src/scripts/popularityMetricLfc/commands/correctGoofs/guessBackfillSpreadDynamicParameters.ts",
    "popular-2-3-free-classes": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/popular23FreeClass/commands/popular23FreeClasses.ts",
    "sorted-numerators": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/popular23FreeClass/commands/sortedNumerators.ts",
    "late-tina-commas": "node -r ts-node/register/transpile-only --max-old-space-size=131072 src/scripts/jiPitch/commands/lateTinaCommas.ts"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.11",
    "@types/node": "^14.0.20",
    "colors": "^1.4.0",
    "commander": "^5.1.0",
    "jasmine": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": ">14"
  }
}
